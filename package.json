    "sass-lint": "1.9.1",
    "sass-loader": "4.0.2",
    "sasslint-webpack-plugin": "1.0.4",
    "style-loader": "0.13.1",
    "ts-loader": "0.9.1",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typed-graphql": "^1.0.2",
    "typescript": "^2.0.3",
