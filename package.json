    "@types/chai": "^3.4.34",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/node": "^6.0.38",
    "codelyzer": "1.0.0-beta.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
