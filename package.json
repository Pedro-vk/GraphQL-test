{
  "name": "graphql-test",
  "version": "0.1.0",
  "private": true,
  "homepage": "graphql.xitrus.es",
  "description": "GraphQL Test",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pedro-vk/GraphQL-test"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rm -rf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install",
    "tslint": "tslint \"./src/**/*.ts\"",
    "sass-lint": "sass-lint --verbose",
    "lint": "npm run sass-lint && npm run tslint",
    "create-cname": "echo $(cat package.json | grep '\"homepage\"' | cut -d '\"' -f 4) > dist/CNAME",
    "gh-pages-travis": "npm run create-cname && gh-pages-travis"
  },
  "dependencies": {
    "@angular/common": "^2.1.0",
