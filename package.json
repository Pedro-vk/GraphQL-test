    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/router": "^3.1.0",
    "@angular/upgrade": "^2.1.0",
    "rxjs": "^5.0.0-rc.1",
    "angular-in-memory-web-api": "^0.1.7",
    "angular2-apollo": "^0.5.0",
    "apollo-client": "^0.4.20",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "es6-shim": "0.35.1",
    "graphql-tag": "^0.1.14",
